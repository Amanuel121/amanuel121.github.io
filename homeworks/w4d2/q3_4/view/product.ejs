<%- include('top.ejs') %>
<a href="/shoppingcart">Shopping Cart</a>
<hr>

<ul>
	<% if( products ) { %>
		<% for (const key in products) { %>
			<li>
				<p>ID: <%= products[key].id %></p>
				<p>Name: <%= products[key].name %></p>
				<p>Price: <%= products[key].price %></p>
				<p>Description: <%= products[key].description %></p>
			</li>
		<% } %>
	<% } %>
</ul>
<%- include('bottom.ejs') %>